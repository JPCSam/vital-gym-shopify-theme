html {
  background-color: #1d1b1b;
}
body.template-customers-account {
  overflow-x: hidden;
  .needsclick[formtype="POPUP"]{
    display: none!important;
  }
  #shopify-section-footer {
    display: none;
  }

  .main-content {
    background-color: #1c1a1a;
    color: #fff;
  }

  input, textarea, button, select, p {
    color: #fff
  }

  h2, h3, h3 a, .section-header__title {
    background: none;
  }

  .tab {
    padding: 30px;
    min-height: calc(100vh - 160px);
    @media(max-width: 992px) {
      padding: 20px 15px;
    }
  }
  .header-layout {
    @media(max-width: 767px) {
      align-items: center;
    }
  }
  .site-header {
    background-color: #000;
    border-color: #2f2f2f;
    padding: 24.5px !important;
    .page-width {
      max-width: 1366px;
      padding: 0 30px;
      @media(max-width: 992px){
        padding: 0 15px;
      }
    }
    .js-search-header {
      display: none;
    }

    .site-navigation {
      margin: 0 0 0 auto;
    }
  }
  .dropdown-tab {
    margin: 0;
    display: none;
    position: absolute;
    right: 0;
    background: white;
    list-style: none;
    z-index: 1;
    padding: 10px 20px;
    min-width: 150px;
    &.dropdown {
      display: block;
    }
  }
  .three-dots {
    position: absolute;
    right: 15px;
    top: 50%;
    transform: translateY(-50%);
    cursor: pointer;
    display: none;
    @media(max-width: 768px){
      display: block;
    }
  }
  .site-nav__icons {
    display: flex;
    flex-direction: row-reverse;
  }
  .header-item--icons {
    flex: 0 0 220px;
    @media(max-width: 768px){
      flex: auto;
      position: absolute;
      left: 0;
      min-width: auto;
    }
  }
  .header-item--logo {
    @media(max-width: 768px){
      flex: auto;
      max-width: none;
    }
  }
  .header-item--navigation{
    @media(max-width: 768px) {
      display: none;
    }
  }
  .site-nav__link {
    color: #fff;
    text-transform: uppercase;
    font-family: 'Industry' !important;
    padding: 5px 24px;

    &.js-drawer-open-cart {
      border-right: 1px solid #2f2f2f;
      border-left: 1px solid #2f2f2f;
      @media(max-width: 768px) {
        display: none;
      }
    }
  }
}

.customer-login {
  position: relative;
  display: flex;
  align-items: center;
  cursor: pointer;
  .button-logout {
    display: none;
    position: absolute;
    background: white;
    padding: 10px;
    width: 110%;
    text-align: center;
    text-transform: none;
    font-weight: 500;
    top: 100%;
    z-index: 1;
    right: 0;
    font-size: 14px;
  }
  &.dropdown .button-logout{
    display: block;
  }
  &:before {
    width: 5px;
    border-top: 1px solid #fff;
    border-left: 1px solid #fff;
    height: 5px;
    content: '';
    display: block;
    transform: rotate(-135deg);
    position: absolute;
    right: 10px;
  }
}

.customer-account-page {
  max-width: 1366px;
  margin: auto;
}

.account-menu {
  display: flex;
  list-style: none;
  margin: 0;
  padding: 0 30px;
  position: relative;
  background-color: #000;
  border-top: 1px solid #2F2F2F;

  &:before {
    background-color: #000;
    content: '';
    position: absolute;
    width: 100vw;
    margin-left: 48%;
    height: 100%;
    transform: translateX(-50%);
    border-bottom: 1px solid #2f2f2f;
  }

  @media(max-width: 992px) {
    padding: 0 15px;
    flex-wrap: wrap;
    align-items: center;
  }

  li {
    z-index: 1;
    padding: 12.5px 15px;
    margin: 0 30px 0 0;
    @media(max-width: 992px) {
      padding: 5px;
      margin: 0 5px;
    }
    &.disabled{
      display: none;
    }
    &.active {
      border-bottom: 2px solid #fff;

      a {
        color: #fff;
        font-weight: 700;
      }
    }
  }
  
  a,span {
    font-size: 16px;
    font-weight: 500;
    font-family: 'Industry' !important;
    color: #666666;
    text-transform: uppercase;
    display: flex;
    @media(max-width: 992px) {
      font-size: 13px;
    }
  }
}

.dashboard {
  .dashboard-slider {
    position: relative;
  }

  .account-slider {
    margin-bottom: 30px;

    .slide {
      position: relative;

      .image-content {
        height: 318px;
        display: block;
        @media (max-width: 992px) {
          position: relative;
          height: auto;
        }
      }
    }

    img {
      width: 100% !important;
      height: 100%;
      position: absolute;
      object-fit: cover;
      @media(max-width: 992px) {
        position: relative;
        max-height: 50vh;
        min-height: 1100px;
      }
      @media(max-width: 767px){
        min-height: 65vh;
      }
      @media(max-width: 475px){
        min-height: 570px;
      }
      @media(max-width: 375px){
        min-height: 510px;
      }
      @media(max-width: 355px){
        min-height: 430px;
      }
    }

    .slide-content {
      display: flex;
      flex-direction: column;
      position: relative;
      z-index: 1;
      padding: 51px 0 109px 46px;
      max-width: 386px;
      @media(max-width: 992px) {
        padding: 0 44px 61px 20px;
        margin-top: -17px;
        background-color: #000;
        max-width: none;
      }
    }

    .subtitle {
      font-size: 14px;
      line-height: 20px;
      font-weight: 500;
      letter-spacing: 0.2em;
      text-transform: uppercase;
      color: #666;
      margin-bottom: 8px;
      @media(max-width: 992px) {
        font-size: 12px;
      }
    }

    .title {
      font-weight: 900;
      font-size: 32px;
      line-height: 36px;
      text-transform: uppercase;
      margin-bottom: 8px;
      @media(max-width: 992px) {
        font-size: 24px;
        line-height: 30px;
      }
    }

    p {
      font-size: 14px;
      line-height: 22px;
      @media(max-width: 992px) {
        font-size: 12px;
        line-height: 18px;
      }
    }
  }

  .slick-navigation {
    position: absolute;
    bottom: 36px;
    width: 100%;
    @media(max-width: 992px) {
      bottom: 28px;
    }

    .slick-dots-custom {
      overflow: hidden;
    }

    .slick-dots {
      position: relative;
      text-align: left;
      left: 46px;
      bottom: 0;
      @media(max-width: 992px) {
        left: 20px;
      }

      li {
        button:before {
          border: none;
          background-color: #fff;
          opacity: 0.5;
          border-radius: 50%;
        }

        &.slick-active button:before {
          opacity: 1;
        }
      }
    }

    .slick-left, .slick-right {
      width: 44px;
      height: 46px;
      display: flex;
      align-content: center;
      justify-content: center;
      background-color: #000;
      border-radius: 0;
      opacity: 1;
      top: 0;
      @media(max-width: 992px) {
        display: none !important;
      }
    }

    .slick-left {
      right: 111px;
    }

    .slick-right {
      right: 47px;
    }
  }

  .dashboard-product {
    background-color: #f0f0f0;
    display: flex;
    padding: 0 45px;
    color: #000;
    width: 49%;
    position: relative;
    @media (min-width: 1366px) {
      width: 638px;
    }
    @media(max-width: 992px) {
      width: 100%;
      padding: 50px 20px 30px;
      margin-bottom: 20px;
      flex-direction: column-reverse;
    }

    .product-count {
      position: absolute;
      right: 0;
      top: 0;
      padding: 10px 16px;
      font-size: 16px;
      line-height: 20px;
      font-family: "Industry" !important;
      font-weight: 500;
      letter-spacing: 0.2em;
      background-color: #000;
      color: #f2c94c;
      @media(max-width: 992px) {
        color: #fff;
      }
    }

    .product-info {
      width: 50%;
      display: flex;
      flex-direction: column;
      padding-right: 65px;
      justify-content: center;
      @media(max-width: 992px) {
        width: 100%;
        margin-top: 10px;
        padding-right: 0;
      }

      h3 {
        font-weight: 500;
        font-size: 14px;
        line-height: 20px;
        letter-spacing: 0.2em;
        color: #666;
        margin-bottom: 8px;
        text-transform: uppercase;
        @media(max-width: 992px) {
          font-size: 12px;
        }
      }

      .product-title {
        font-weight: 900;
        font-size: 32px;
        line-height: 36px;
        text-transform: uppercase;
        @media(max-width: 992px) {
          font-size: 24px;
        }
      }

      .product-price {
        margin-bottom: 20px;
        display: flex;
        align-items: center;
        flex-wrap: wrap;
      }

      .product__price {
        font-family: 'Industry' !important;
        font-weight: bold;

        &.sale-price {
          font-size: 26px;
          @media(max-width: 992px) {
            font-size: 24px;
          }
        }
      }

      .product__price--compare {
        font-weight: 500;
        font-size: 20px;
        margin-right: 20px;
      }

      .btn {
        // padding: 12px 24px!important;
        // height: 46px;
        font-size: 16px;
        @media(max-width: 992px) {
          max-width: none;
        }
      }
    }

    .product-image {
      width: 50%;
      display: flex;
      justify-content: center;
      @media(max-width: 992px) {
        width: 100%;
      }

      img {
        width: 100%;
        max-width: 272px;
        object-fit: contain;
        max-height: 272px;
        margin-top: 30px;
      }
    }
  }

  .dashboard-bottom {
    display: flex;
    justify-content: space-between;
    @media(max-width: 992px) {
      flex-direction: column;
    }
  }

  .dashboard-gift {
    width: calc(51% - 30px);
    display: flex;
    justify-content: space-between;
    @media(min-width: 1366px){
      width: calc(100% - 668px);
    }
    @media(max-width: 992px) {
      width: 100%;
      flex-direction: column;
    }

    .gift-container {
      width: calc(50% - 15px);
      display: flex;
      flex-direction: column;
      align-items: center;
      text-align: center;
      @media(max-width: 992px) {
        width: 100%;
        margin-bottom: 20px;
      }

      h3 {
        font-weight: 500;
        font-size: 10px;
        line-height: 16px;
        letter-spacing: 0.2em;
        text-transform: uppercase;
        color: #ffcc00;
        margin-bottom: 6px;
        @media(max-width: 992px) {
          font-size: 12px;
        }
      }
      
      .copied-alert {
        height: 25px;
        opacity: 0;
      }
      
      .copied-alert-text {
        font-size: 12px;
        line-height: 12px;
        margin-bottom: 0px;
        letter-spacing: 0;
        text-transform: none;
        vertical-align: middle;
      }
    }

    .give-away {
      padding: 25px 20px 0;
      background-color: #000;

      h2 {
        font-size: 24px;
        text-transform: uppercase;
        line-height: 30px;
        margin-bottom: 30px;
        font-family: "Industry"!important;
        font-weight: 900;
      }

      img {
        height: 100%;
        object-fit: cover;
      }
    }

    .discount-code {
      margin-bottom: 15px;
    }

    .discount-id {
      display: flex;
      align-items: center;
      background: #1C1A1A;
      width: 100%;
      position: relative;
      justify-content: center;
      padding: 16px;

      #discount-copy {
        position: absolute;
        right: 15px;
      }

      #copy_discount {
        margin: 0;
        font-weight: bold;
        font-family: 'Industry' !important;
        font-size: 16px;
        line-height: 21px;
        border: none;
        padding: 0;
        text-align: center;
      }
    }

    .discount-code {
      display: flex;
      align-items: center;
      padding: 25px 20px 0px;
      flex-direction: column;
      background-color: #000;
      width: 100%;

      h2 {
        font-size: 20px;
        line-height: 24px;
        text-transform: uppercase;
        font-family: "Industry"!important;
        font-weight: 900;
        @media(max-width: 992px) {
          font-size: 24px;
          line-height: 30px;
        }
      }
    }

    .spotify {
      display: flex;
      background-color: #000;
      overflow: hidden;
      height: 100%;
      padding: 47px 0;
      width: 100%;
      background-image:url({{ 'spotify.png' | asset_url}});
      background-size: contain;
      justify-content: flex-end;
      background-repeat: no-repeat;
      .text-spotify {
        display: flex;
        flex-direction: column;
        align-items: baseline;
        justify-content: center;
        width: calc(100% - 166px);
      }
      h2 {
        font-size: 20px;
        line-height: 24px;
        text-transform: uppercase;
        text-align: left;
        margin-bottom: 0;
        color: #fff;
        font-weight: 700;
        font-family: "Industry"!important;
        @media(max-width: 992px) {
          font-size: 24px;
          line-height: 30px;
        }
      }
    }
  }
}

.vital-perks {
  .vital-visit {
    display: flex;
    position: relative;
    margin-bottom: 30px;
    background-color: #000;
    @media(max-width: 992px) {
      flex-direction: column;
    }

    .percent {
      position: absolute;
      left: 0;
      top: 0;
      font-family: 'Industry';
      font-style: normal;
      font-weight: 500;
      font-size: 16px;
      line-height: 20px;
      background-color: #000;
      padding: 10px 15px;
      text-transform: uppercase;
      letter-spacing: 0.2em;
    }

    .vital-image {
      width: 24%;
      @media(max-width: 992px) {
        width: 100%;
      }

      img {
        display: block;
        width: 100%;
        height:100%;
        object-fit: cover;
      }
    }

    .vital-content {
      width: 76%;
      display: flex;
      align-items: center;
      padding: 0 60px 0 35px;
      justify-content: space-between;
      @media(max-width: 992px) {
        flex-direction: column;
        width: 100%;
        padding: 20px 20px 24px;
      }
    }

    .vital-text {
      display: flex;
      flex-direction: column;
      max-width: 534px;
      padding: 30px 0;
      @media (max-width: 992px) {
        max-width: none;
        margin-bottom: 24px;
        width: 100%;
        padding: 0;
      }

      h3 {
        font-size: 16px;
        line-height: 20px;
        letter-spacing: 0.2em;
        margin-bottom: 8px;
        text-transform: uppercase;
        font-weight: 500;
        color: #666;
        @media (max-width: 992px) {
          font-size: 12px;
          line-height: 20px;
        }
      }

      h2 {
        font-size: 26px;
        line-height: 32px;
        text-transform: uppercase;
        margin-bottom: 8px;
        @media (max-width: 992px) {
          font-size: 24px;
          line-height: 30px;
        }
      }

      .description-content p {
        font-weight: 400;
        font-size: 16px;
        line-height: 24px;
        font-family: 'Industry' !important;
        letter-spacing: -0.01em;
        margin-bottom: 0;
        @media (max-width: 992px) {
          font-size: 12px;
          line-height: 18px;
        }
      }
    }

    .vital-button {
      display: flex;
      align-items: center;
      flex-direction: column;
      text-align: center;
      max-width: 190px;
      @media (max-width: 992px) {
        width: 100%;
        max-width: 500px;
      }

      .btn {
        padding: 12px 0 !important;
        display: flex;
        align-items: center;
        min-width: 163px;
        margin-bottom: 16px;
        justify-content: center;
        @media (max-width: 992px) {
          width: 100%;
          margin-bottom: 8px;
        }
      }

      p {
        font-size: 15px;
        font-weight: 400;
        font-family: 'Industry' !important;
        color: #eee;
        letter-spacing: 0.01em;

        span {
          font-weight: bold;
        }
      }
    }
  }
}

